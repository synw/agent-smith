{
  "name": "@agent-smith/cli",
  "description": "Agent Smith: terminal client for language model agents",
  "repository": "https://github.com/synw/agent-smith",
  "version": "0.0.36",
  "scripts": {
    "buildrl": "rm -rf dist/* && rollup -c",
    "build": "rm -rf dist/* && tsc --noCheck",
    "cli": "node --loader ts-node/esm bin/index.ts",
    "watch": "tsc --noCheck -p . -w"
  },
  "dependencies": {
    "@agent-smith/brain": "^0.0.38",
    "@agent-smith/jobs": "^0.0.13",
    "@agent-smith/lmtask": "^0.0.31",
    "@agent-smith/tfm": "^0.1.1",
    "@inquirer/prompts": "^7.3.3",
    "@inquirer/select": "^4.0.10",
    "@vue/reactivity": "^3.5.13",
    "better-sqlite3": "^11.8.1",
    "clipboardy": "^4.0.0",
    "commander": "^13.1.0",
    "marked-terminal": "^7.3.0",
    "modprompt": "^0.10.4",
    "python-shell": "^5.0.0",
    "yaml": "^2.7.0"
  },
  "devDependencies": {
    "@agent-smith/tmem-jobs": "^0.0.4",
    "@commander-js/extra-typings": "^13.1.0",
    "@locallm/types": "^0.1.5",
    "@rollup/plugin-node-resolve": "^16.0.0",
    "@rollup/plugin-typescript": "^12.1.2",
    "@types/better-sqlite3": "^7.6.12",
    "@types/marked-terminal": "^6.1.1",
    "@types/node": "^22.13.10",
    "restmix": "^0.5.0",
    "rollup": "^4.35.0",
    "ts-node": "^10.9.2",
    "tslib": "2.8.1",
    "typescript": "^5.8.2"
  },
  "type": "module",
  "preferGlobal": true,
  "main": "./dist/main.js",
  "exports": {
    ".": {
      "import": "./dist/main.js"
    }
  },
  "files": [
    "dist"
  ],
  "bin": {
    "lm": "./dist/index.js"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "license": "MIT"
}