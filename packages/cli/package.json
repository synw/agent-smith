    "@types/better-sqlite3": "^7.6.11",
    "@types/marked-terminal": "^6.1.1",
    "@types/node": "^22.5.1",
    "rollup": "^4.21.1",
    "ts-node": "^10.9.2",
    "tslib": "2.7.0",
    "typescript": "^5.5.4"
