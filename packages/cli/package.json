    "@vue/reactivity": "^3.5.12",
    "better-sqlite3": "^11.5.0",
    "clipboardy": "^4.0.0",
    "commander": "^12.1.0",
    "draftlog": "^1.0.13",
    "marked-terminal": "^7.2.1",
    "modprompt": "^0.9.1",
