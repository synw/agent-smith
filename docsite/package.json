    "@agent-smith/tmem": "^0.0.4",
    "@agent-smith/tmem-jobs": "^0.0.4",
    "@docdundee/vue": "^0.0.21",
    "@locallm/api": "^0.2.0",
    "@locallm/browser": "^0.0.21",
    "@nanostores/vue": "^0.11.0",
    "@primevue/themes": "^4.3.3",
    "@snowind/header": "^0.1.0",
    "highlight.js": "^11.11.1",
    "markdown-it": "^14.1.0",
    "modprompt": "^0.10.9",
