    "@tailwindcss/forms": "^0.5.9",
    "@tailwindcss/typography": "^0.5.15",
    "@types/node": "^22.5.4",
    "@vitejs/plugin-vue": "^5.1.3",
    "@vue/compiler-sfc": "^3.5.3",
    "autoprefixer": "^10.4.20",
    "gzipper": "^8.1.0",
    "npm-run-all": "^4.1.5",
