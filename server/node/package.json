{
  "name": "@agent-smith/server",
  "version": "0.0.2",
  "description": "Agent Smith Nodejs server",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "rm -rf dist && tsc",
    "start": "node dist/index.js",
    "server": "node --loader ts-node/esm src/index.ts",
    "devserver": "node  --loader ts-node/esm --watch src/index.ts"
  },
  "type": "module",
  "dependencies": {
    "@agent-smith/cli": "^0.0.111",
    "@koa/cors": "^5.0.0",
    "@koa/router": "^15.3.0",
    "ansi-colors": "^4.1.3",
    "koa": "^3.1.1",
    "koa-bodyparser": "^4.4.1",
    "koa-route": "^4.0.1",
    "koa-static": "^5.0.0",
    "koa-websocket": "^7.0.0"
  },
  "devDependencies": {
    "@agent-smith/types": "^0.0.2",
    "@locallm/types": "^0.7.0",
    "@types/better-sqlite3": "^7.6.13",
    "@types/koa": "^3.0.1",
    "@types/koa__cors": "^5.0.1",
    "@types/koa-bodyparser": "^4.3.13",
    "@types/koa-route": "^3.2.9",
    "@types/koa-static": "^4.0.4",
    "@types/koa-websocket": "^5.0.11",
    "@types/node": "^25.3.0",
    "ts-node": "^10.9.2",
    "tslib": "2.8.1",
    "typescript": "^5.9.3"
  },
  "preferGlobal": true,
  "main": "./dist/main.js",
  "types": "./dist/main.d.ts",
  "exports": {
    ".": {
      "import": "./dist/main.js"
    }
  },
  "files": [
    "dist"
  ],
  "bin": {
    "agentserver": "./dist/index.js"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "license": "MIT"
}